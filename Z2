print('Введите необходимую функцию: сложение, вычитание, умножение, деление, возведение в степень, логарифм, округление в большую сторону до N знака после запятой, округление в меньшую сторону до N знака после запятой') 
a=input() 
if a=='сложение': 
 print("Введите первый элемент") 
 b=int(input()) 
 print("Введите второй элемент") 
 c=int(input()) 
 print(b+c) 
elif a=='вычитание': 
 print("Введите первый элемент") 
 b=int(input()) 
 print("Введите второй элемент") 
 c=int(input()) 
 print(b-c) 
elif a=='умножение': 
 print("Введите первый элемент") 
 b=int(input()) 
 print("Введите второй элемент") 
 c=int(input()) 
 print(b*c) 
elif a=='деление': 
 print("Введите первый элемент") 
 b=int(input()) 
 print("Введите второй элемент") 
 c=int(input()) 
 print(b/c) 
elif a=='возведение в степень': 
 print("Введите первый элемент") 
 b=int(input()) 
 print("Введите второй элемент") 
 c=int(input()) 
 print(b**c) 
elif a=='логарифм': 
 print("Введите первый элемент") 
 b=int(input()) 
 print("Введите основание логарифма") 
 c=int(input()) 
 import math 
 print(math.log(b,c)) 
elif a=='округление в большую сторону до N знака после запятой': 
 d=float(input('Введите число')) 
 b=int(input('Введите N')) 
 c=d*(10**b) 
 import math 
 print(math.ceil(c)/(10**b)) 
elif a=='округление в меньшую сторону до N знака после запятой': 
 d=float(input('Введите число')) 
 b=int(input('Введите N')) 
 c=d*(10**b) 
 import math 
 print(math.floor(c)/(10**b))
